{
  "name": "nuxt-environment",
  "version": "1.0.0",
  "description": "Module to quickly get information about the current environment that the site is running in",
  "main": "lib/module.js",
  "scripts": {
    "dev": "nuxt test/fixture",
    "lint": "eslint lib test",
    "test": "yarn lint && jest",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/jaydenrasmussen/nuxt-environment",
  "files": [
    "lib"
  ],
  "keywords": [
    "nuxt",
    "environment",
    "browser"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaydenrasmussen/nuxt-environment/issues"
  },
  "homepage": "https://github.com/jaydenrasmussen/nuxt-environment#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@nuxtjs/eslint-config": "0.0.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "nuxt-edge": "^2.6.2-25908504.a9d2deb1"
  },
  "dependencies": {
    "bowser": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "git add"
    ],
    "ignore": [
      "babel.config.js",
      "jest.config.js"
    ]
  }
}
